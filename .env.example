# MIS Configuration
MIS_API_ENDPOINT=http://localhost:8000
MIS_API_TOKEN=your-mis-token
MIS_WEBHOOK_SECRET=your-webhook-secret

# zen-MCP Configuration
ZEN_MCP_API_KEY=your-zen-mcp-api-key
ZEN_MCP_ENDPOINT=https://api.zen-mcp.com
ZEN_MCP_TIMEOUT=30

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/mis_zenmcp
REDIS_URL=redis://localhost:6379/0

# Application Settings
APP_ENV=development
APP_DEBUG=true
APP_PORT=8001
APP_HOST=0.0.0.0

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json

# Performance
MAX_WORKERS=4
REQUEST_TIMEOUT=60
CACHE_TTL=3600

# Security
SECRET_KEY=your-secret-key-here
ALLOWED_HOSTS=localhost,127.0.0.1

# Monitoring
PROMETHEUS_PORT=9090
ENABLE_TRACING=true
JAEGER_ENDPOINT=http://localhost:14268/api/traces